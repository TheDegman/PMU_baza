<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PMU_baza.UNOS">
    <ContentPage.Content>
        <StackLayout>
            <DatePicker></DatePicker>
            <Entry Placeholder="Struja"
                   x:Name="Struja"
                  ></Entry>
            <Entry Placeholder="Plin"
                   x:Name="Plin"
                   ></Entry>
            <Entry Placeholder="TV"
                   x:Name="TV"
                   ></Entry>
            <Entry Placeholder="Voda"
                   x:Name="Voda"></Entry>
            <Button Text="Unos"
                    x:Name="Unos"
                    Clicked="Unos_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>